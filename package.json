{
  "name": "aptugoclient",
  "version": "1.5.1",
  "description": "Command line client for Aptugo",
  "engines": {
    "node": ">=14.16"
  },
  "preferGlobal": true,
  "bin": {
    "aptugo": "./bin/index.js"
  },
  "main": "./index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config webpack.config.cjs",
    "pub": "npm version patch --force && npm publish",
    "postinstall": "node ./post-install.js",
    "rebuild": "electron-rebuild -f -w node-pty"
  },
  "author": "Aptugo",
  "license": "LGPL-3.0-or-later",
  "pkg": {
    "scripts": "bin/index.js",
    "outputPath": "dist"
  },
  "browserslist": "> 0.25%, not dead",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "adm-zip": "^0.5.9",
    "axios": "^0.21.1",
    "boxen": "^5.0.1",
    "chalk": "^2.4.2",
    "cli-select": "^1.1.2",
    "cli-spinners": "^2.6.0",
    "cli-table": "^0.3.6",
    "conf": "^9.0.2",
    "execa": "^6.1.0",
    "form-data": "^4.0.0",
    "formdata-node": "^4.3.2",
    "gently-copy": "^3.2.0",
    "humanize-duration": "^3.25.1",
    "image-size": "^1.0.1",
    "istextorbinary": "^6.0.0",
    "js-yaml": "^4.0.0",
    "minimist": "^1.2.0",
    "node-pty": "^0.10.1",
    "npm-run-path": "^5.1.0",
    "ora": "^5.4.0",
    "ora-promise": "^1.0.0",
    "original-fs": "^1.1.0",
    "path-key": "^4.0.0",
    "prettier": "^2.2.1",
    "prettier-plugin-organize-imports": "^1.1.1",
    "prompt": "^1.1.0",
    "sharp": "^0.30.7",
    "splitargs": "0.0.7",
    "strip-ansi": "^7.0.1",
    "twig": "^1.15.4",
    "typescript": "^4.4.3",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@trivago/prettier-plugin-sort-imports": "^2.0.4",
    "babel-loader": "^8.2.5",
    "electron-rebuild": "^3.2.7"
  }
}
